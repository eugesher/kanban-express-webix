{
  "name": "kanban-express-webix",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start": "node dist/app.js",
    "dev:build": "tsc -w --preserveWatchOutput",
    "dev:start": "nodemon dist/app.js",
    "dev": "concurrently npm:dev:*",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config=src/config/ormconfig.ts",
    "db:drop": "npm run typeorm schema:drop",
    "db:create": "npm run typeorm migration:generate -- -n",
    "db:migrate": "npm run typeorm migration:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "passport": "^0.5.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.38"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.1",
    "@types/passport": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "concurrently": "^6.3.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.4.4"
  }
}
