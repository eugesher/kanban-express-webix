{
  "name": "kanban-express-webix",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:server": "tsc && cp -r ./public ./dist",
    "build:app": "webpack",
    "build": "concurrently npm:build:*",
    "start": "node dist/server/app.js",
    "dev:build": "tsc -w --preserveWatchOutput",
    "dev:start": "nodemon dist/server/app.js",
    "dev:app": "webpack-dev-server --open",
    "dev": "concurrently npm:dev:*",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config=server/config/ormconfig.ts",
    "db:drop": "npm run typeorm schema:drop",
    "db:create": "npm run typeorm migration:generate -- -n",
    "db:migrate": "npm run typeorm migration:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
